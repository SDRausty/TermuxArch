while true;do echo playing play-audio "$@";play-audio "$@";snooze="$(cat /proc/sys/kernel/random/uuid | sed 's/[^0-9]//g'|sed 's/^\(...\).*/\1/')";echo snoozing play-audio "$@" for $snooze seconds;sleep $snooze;done
