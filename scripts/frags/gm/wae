while true;do printplay "$@";play-audio "$@" 1>/dev/null; (( snooze="$(sed 's/[^0-9]//g' /proc/sys/kernel/random/uuid |sed 's/^\(...\).*/\1/')" )) ;printsnooze "$@";sleep "$snooze";done
